				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="sys-claims-cms-config" attributeName="config-ref" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:error typeId="APIKIT:NOT_ACCEPTABLE" />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<try doc:name="Try" doc:id="89b6e104-5cde-4deb-a123-6d3b27e5b3de" >
				<flow-ref doc:name="Flow Reference" doc:id="b5222829-3f8c-40da-b706-9714b48c5570" name="sys-claims-cms-main"/>
